<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane fx:id="rootPane" prefHeight="500.0" prefWidth="710.0" stylesheets="@mainWindowStyles.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="chatpsit.adminpanel.MainWindowController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#logoutAndQuitToLogin" text="Logout" />
              <MenuItem mnemonicParsing="false" onAction="#logoutAndCloseWindow" text="Esci" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="?">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#showInfoDialog" text="Info su" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <center>
      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="471.0" prefWidth="241.0" />
   </center>
   <left>
      <ListView fx:id="sidebarMenu" items="${controller.sidebarEntries}" prefHeight="471.0" prefWidth="170.0" BorderPane.alignment="CENTER" />
   </left>
</BorderPane>
